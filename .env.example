# URL da Evolution API
NEXT_PUBLIC_EVOLUTION_URL=http://localhost:8080

# Chave API da Evolution
NEXT_PUBLIC_EVOLUTION_API=your_api_key_here

# URL da aplicação
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Credenciais de login para o painel
# IMPORTANTE: Mude estes valores em produção!
NEXT_PUBLIC_AUTH_USERNAME=admin
NEXT_PUBLIC_AUTH_PASSWORD=admin

# Chave secreta para JWT (mude em produção!)
AUTH_SECRET_KEY=secret_key_change_this

# Integração com Chatwoot (opcional)
NEXT_PUBLIC_CHATWOOT_INTEGRATION=false
NEXT_PUBLIC_CHATWOOT_DOMAIN=
NEXT_PUBLIC_CHATWOOT_ACCOUNT_ID=
NEXT_PUBLIC_CHATWOOT_TOKEN=
NEXT_PUBLIC_CHATWOOT_DATABASE_CONNECTION_URI=

# Configurações regionais
NEXT_PUBLIC_AUTO_FILL_DDI=true
NEXT_PUBLIC_DEFAULT_LOCATION=BR